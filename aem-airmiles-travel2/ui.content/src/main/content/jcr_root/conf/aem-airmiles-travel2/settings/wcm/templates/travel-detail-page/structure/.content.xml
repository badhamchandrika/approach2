<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0"
          xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
          jcr:primaryType="cq:Page">
    <jcr:content
            cq:deviceGroups="[mobile/groups/responsive]"
            cq:lastModified="{Date}2022-11-04T15:10:55.540-04:00"
            cq:lastModifiedBy="admin"
            cq:template="/conf/aem-airmiles-travel2/settings/wcm/templates/travel-detail-page"
            jcr:primaryType="cq:PageContent"
            sling:resourceType="aem-airmiles-travel2/components/structure/dealsdetailpage">
        <root
                jcr:primaryType="nt:unstructured"
                sling:resourceType="wcm/foundation/components/responsivegrid">
            <experiencefragment-header
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="aem-airmiles-web/components/experiencefragment"
                    fragmentVariationPath="/content/experience-fragments/aem-airmiles-travel2/ca/en/site/header/travel2"/>
            <container
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="aem-airmiles-travel2/components/container"
                    layout="responsiveGrid">
                <container
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="aem-airmiles-travel2/components/container"
                        editable="{Boolean}true"
                        layout="responsiveGrid"/>
            </container>
            <experiencefragment-footer
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="aem-airmiles-web/components/experiencefragment"
                    fragmentVariationPath="/content/experience-fragments/aem-airmiles-travel2/ca/en/site/footer/travel"/>
        </root>
        <cq:responsive jcr:primaryType="nt:unstructured">
            <breakpoints jcr:primaryType="nt:unstructured">
                <phone
                        jcr:primaryType="nt:unstructured"
                        title="Smaller Screen"
                        width="{Long}650"/>
                <tablet
                        jcr:primaryType="nt:unstructured"
                        title="Tablet"
                        width="{Long}1200"/>
            </breakpoints>
        </cq:responsive>
    </jcr:content>
</jcr:root>
