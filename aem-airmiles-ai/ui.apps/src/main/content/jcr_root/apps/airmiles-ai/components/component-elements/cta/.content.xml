<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    sling:resourceType="granite/ui/components/coral/foundation/container"
    margin="{Boolean}true">
    <items jcr:primaryType="nt:unstructured">
        <ctaAccordion
            jcr:primaryType="nt:unstructured"
            sling:resourceType="granite/ui/components/coral/foundation/accordion"
            margin="{Boolean}false"
            maximized="{Boolean}true"
            multiple="{Boolean}true"
            variant="quiet">
            <items jcr:primaryType="nt:unstructured">
                <cta
                    jcr:primaryType="nt:unstructured"
                    jcr:title="CTA"
                    sling:resourceType="granite/ui/components/foundation/container">
                    <items jcr:primaryType="nt:unstructured">
                        <ctaText
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                            fieldLabel="CTA Label"
                            name="./cta/ctaText"/>
                        <ctaUrl
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                            emptyText="Url"
                            fieldLabel="URL"
                            name="./cta/ctaUrl"/>
                        <ctaIcon
				            jcr:primaryType="nt:unstructured"
				            sling:resourceType="granite/ui/components/coral/foundation/form/select"
				            emptyText="Please select a Icon"
				            fieldLabel="Select the Icon:"
				            name="./cta/ctaIcon">
				           	<items jcr:primaryType="nt:unstructured">
				                <globe
					                jcr:primaryType="nt:unstructured"
					                text="Globe"
					                value="am-icon-globe"
					                icon="am-icon-globe"/>
				               	<arrow-left
					                jcr:primaryType="nt:unstructured"
					                text="Arrow Left"
					                value="am-icon-arrow-left"
					                icon="am-icon-arrow-left"/>
					            <active-arrow-left
					                jcr:primaryType="nt:unstructured"
					                text="Active Arrow Left"
					                value="am-icon-active-arrow-left"
					                icon="am-icon-active-arrow-left"/>	                
				                <arrow-right
					                jcr:primaryType="nt:unstructured"
					                text="Arrow Right"
					                value="am-icon-arrow-right"
					                icon="am-icon-arrow-right"/>
					            <active-arrow-right
					                jcr:primaryType="nt:unstructured"
					                text="Active Arrow Right"
					                value="am-icon-active-arrow-right"
					                icon="am-icon-active-arrow-right"/> 	                              
				                <graph-time
					                jcr:primaryType="nt:unstructured"
					                text="Graph Time"
					                value="am-icon-graph-time"
					                icon="am-icon-graph-time"/>
				                <time-circle
					                jcr:primaryType="nt:unstructured"
					                text="Time Circle"
					                value="am-icon-graph-time-circle"
					                icon="am-icon-graph-time-circle"/>
				                <check
					                jcr:primaryType="nt:unstructured"
					                text="Check"
					                value="am-icon-check"
					                icon="am-icon-check"/>
					            <outline
					                jcr:primaryType="nt:unstructured"
					                text="Outline"
					                value="am-icon-check-outline"
					                icon="am-icon-check-outline"/>	                
				                <pin
					                jcr:primaryType="nt:unstructured"
					                text="Pin"
					                value="am-icon-pin"
					                icon="am-icon-pin"/>
				                <graphic
					                jcr:primaryType="nt:unstructured"
					                text="Graphic"
					                value="am-icon-graphic"
					                icon="am-icon-graphic"/>
				                <idea
					                jcr:primaryType="nt:unstructured"
					                text="Idea"
					                value="am-icon-idea"
					                icon="am-icon-idea"/>
				                <presentation
					                jcr:primaryType="nt:unstructured"
					                text="Presentation"
					                value="am-icon-presentation"
					                icon="am-icon-presentation"/>
					            <share
					                jcr:primaryType="nt:unstructured"
					                text="Share"
					                value="am-icon-share"
					                icon="am-icon-share"/>
					            <chev-left
					                jcr:primaryType="nt:unstructured"
					                text="Chev Left"
					                value="am-icon-chev-left"
					                icon="am-icon-chev-left"/>
				                <chev-right
					                jcr:primaryType="nt:unstructured"
					                text="Chev Right"
					                value="am-icon-chev-right"
					                icon="am-icon-chev-right"/>
				                <chev-down
					                jcr:primaryType="nt:unstructured"
					                text="Chev Down"
					                value="am-icon-chev-down"
					                icon="am-icon-chev-down"/>
				                <chev-up
					                jcr:primaryType="nt:unstructured"
					                text="Chev Up"
					                value="am-icon-chev-up"
					                icon="am-icon-chev-up"/>
				                <dialog
					                jcr:primaryType="nt:unstructured"
					                text="Dialog"
					                value="am-icon-dialog"
					                icon="am-icon-dialog"/>
				                <menu
					                jcr:primaryType="nt:unstructured"
					                text="Menu"
					                value="am-icon-menu"
					                icon="am-icon-menu"/>
				                <logo
					                jcr:primaryType="nt:unstructured"
					                text="Logo"
					                value="am-icon-logo"
					                icon="am-icon-logo"/> 
				            </items>
				        </ctaIcon>                        
                        <ctaTarget
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/coral/foundation/form/select"
                            fieldLabel="Target"
                            name="./cta/ctaTarget">
                            <items jcr:primaryType="nt:unstructured">
                                <sameTab
                                    jcr:primaryType="nt:unstructured"
                                    text="Same Tab"
                                    value="_self"/>
                                <newTab
                                    jcr:primaryType="nt:unstructured"
                                    text="New Tab"
                                    value="_blank"/>
                                <parentFrame
                                    jcr:primaryType="nt:unstructured"
                                    text="Parent Frame"
                                    value="_parent"/>
                                <topFrame
                                    jcr:primaryType="nt:unstructured"
                                    text="Top Frame"
                                    value="_top"/>
                            </items>
                        </ctaTarget>
                    </items>
                </cta>
            </items>
        </ctaAccordion>
    </items>
</jcr:root>
