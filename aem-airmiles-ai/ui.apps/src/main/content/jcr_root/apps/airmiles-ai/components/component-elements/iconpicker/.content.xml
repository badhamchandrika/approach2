<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    sling:resourceType="granite/ui/components/coral/foundation/container"
    margin="{Boolean}true">
    <items jcr:primaryType="nt:unstructured">
        <icon
            jcr:primaryType="nt:unstructured"
            sling:resourceType="granite/ui/components/coral/foundation/form/select"
            emptyText="Please select a Icon"
            fieldLabel="Select the Icon:"
            name="./icon">
           	<items jcr:primaryType="nt:unstructured">
                <globe
	                jcr:primaryType="nt:unstructured"
	                text="Globe"
	                value="am-icon-globe"
	                icon="am-icon-globe"/>
               	<arrow-left
	                jcr:primaryType="nt:unstructured"
	                text="Arrow Left"
	                value="am-icon-arrow-left"
	                icon="am-icon-arrow-left"/>
	            <active-arrow-left
	                jcr:primaryType="nt:unstructured"
	                text="Active Arrow Left"
	                value="am-icon-active-arrow-left"
	                icon="am-icon-active-arrow-left"/>	                
                <arrow-right
	                jcr:primaryType="nt:unstructured"
	                text="Arrow Right"
	                value="am-icon-arrow-right"
	                icon="am-icon-arrow-right"/>
	            <active-arrow-right
	                jcr:primaryType="nt:unstructured"
	                text="Active Arrow Right"
	                value="am-icon-active-arrow-right"
	                icon="am-icon-active-arrow-right"/> 	                              
                <graph-time
	                jcr:primaryType="nt:unstructured"
	                text="Graph Time"
	                value="am-icon-graph-time"
	                icon="am-icon-graph-time"/>
                <time-circle
	                jcr:primaryType="nt:unstructured"
	                text="Time Circle"
	                value="am-icon-graph-time-circle"
	                icon="am-icon-graph-time-circle"/>
                <check
	                jcr:primaryType="nt:unstructured"
	                text="Check"
	                value="am-icon-check"
	                icon="am-icon-check"/>
	            <outline
	                jcr:primaryType="nt:unstructured"
	                text="Outline"
	                value="am-icon-check-outline"
	                icon="am-icon-check-outline"/>	                
                <pin
	                jcr:primaryType="nt:unstructured"
	                text="Pin"
	                value="am-icon-pin"
	                icon="am-icon-pin"/>
                <graphic
	                jcr:primaryType="nt:unstructured"
	                text="Graphic"
	                value="am-icon-graphic"
	                icon="am-icon-graphic"/>
                <idea
	                jcr:primaryType="nt:unstructured"
	                text="Idea"
	                value="am-icon-idea"
	                icon="am-icon-idea"/>
                <presentation
	                jcr:primaryType="nt:unstructured"
	                text="Presentation"
	                value="am-icon-presentation"
	                icon="am-icon-presentation"/>
	            <share
	                jcr:primaryType="nt:unstructured"
	                text="Share"
	                value="am-icon-share"
	                icon="am-icon-share"/>
	            <chev-left
	                jcr:primaryType="nt:unstructured"
	                text="Chev Left"
	                value="am-icon-chev-left"
	                icon="am-icon-chev-left"/>
                <chev-right
	                jcr:primaryType="nt:unstructured"
	                text="Chev Right"
	                value="am-icon-chev-right"
	                icon="am-icon-chev-right"/>
                <chev-down
	                jcr:primaryType="nt:unstructured"
	                text="Chev Down"
	                value="am-icon-chev-down"
	                icon="am-icon-chev-down"/>
                <chev-up
	                jcr:primaryType="nt:unstructured"
	                text="Chev Up"
	                value="am-icon-chev-up"
	                icon="am-icon-chev-up"/>
                <dialog
	                jcr:primaryType="nt:unstructured"
	                text="Dialog"
	                value="am-icon-dialog"
	                icon="am-icon-dialog"/>
                <menu
	                jcr:primaryType="nt:unstructured"
	                text="Menu"
	                value="am-icon-menu"
	                icon="am-icon-menu"/>
                <logo
	                jcr:primaryType="nt:unstructured"
	                text="Logo"
	                value="am-icon-logo"
	                icon="am-icon-logo"/>              
            </items>
        </icon>
    </items>
</jcr:root>
