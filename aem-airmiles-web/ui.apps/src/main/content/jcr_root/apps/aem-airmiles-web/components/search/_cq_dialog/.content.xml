<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
          jcr:primaryType="nt:unstructured"
          jcr:title="Quick Search"
          sling:resourceType="cq/gui/components/authoring/dialog">
    <content
            jcr:primaryType="nt:unstructured"
            sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/coral/foundation/tabs"
                    maximized="{Boolean}true">
                <items jcr:primaryType="nt:unstructured">
                    <properties
                            jcr:primaryType="nt:unstructured"
                            jcr:title="Properties"
                            sling:resourceType="granite/ui/components/coral/foundation/container"
                            margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                    margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <searchLabel
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    fieldDescription="Search Label"
                                                    fieldLabel="Search Label"
                                                    name="./searchLabel"/>
                                            <cancelLabel
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    fieldDescription="Cancel Label"
                                                    fieldLabel="Cancel Label"
                                                    name="./cancelLabel"/>
                                            <submitLabel
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    fieldDescription="Cancel Label"
                                                    fieldLabel="Submit Label"
                                                    name="./submitLabel"/>
                                            <searchPlaceholderText
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    fieldDescription="Search Placeholder Text"
                                                    fieldLabel="Search Placeholder Text"
                                                    name="./searchPlaceholderText"/>
                                            <pagination
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/numberfield"
                                                    defaultValue="10"
                                                    fieldLabel="Number of result shown on the page (Pagination)"
                                                    max="100"
                                                    min="1"
                                                    name="./pagination"
                                                    step="1"/>
                                            <searchResultPage
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="cq/gui/components/coral/common/form/pagefield"
                                                    fieldDescription="Redirect search result to selected page."
                                                    fieldLabel="Search Result Page"
                                                    name="./searchResultPage"
                                                    rootPath="/content"
                                                    required="{Boolean}true"/>
                                            <serachbuttontrackid
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    fieldLabel="Search Button - Analytics Tracking ID"
                                                    name="./searchButtonTrackID"/>
                                            <serachbuttonclickid
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    fieldLabel="Search Button - Analytics Click ID"
                                                    name="./searchButtonClickID"/>
                                            <serachbuttontracktype
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                                    emptyText="Track Type"
                                                    fieldLabel="Search Button - Analytics Tracking Type"
                                                    name="./searchButtonTrackType">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <internal
                                                            jcr:primaryType="nt:unstructured"
                                                            selected="{Boolean}true"
                                                            text="Internal"
                                                            value="internal"/>
                                                    <button
                                                            jcr:primaryType="nt:unstructured"
                                                            text="Button"
                                                            value="button"/>
                                                    <exit
                                                            jcr:primaryType="nt:unstructured"
                                                            text="Exit"
                                                            value="exit"/>
                                                    <social
                                                            jcr:primaryType="nt:unstructured"
                                                            text="Social"
                                                            value="social"/>
                                                </items>
                                            </serachbuttontracktype>
                                            <searchinputtrackid
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    fieldLabel="Search Input - Analytics Track ID"
                                                    name="./searchInputTrackID"/>
                                            <searchinputclickid
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    fieldLabel="Search Input - Analytics Click ID"
                                                    name="./searchInputClickID"/>
                                            <searchinputtracktype
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                                    emptyText="Track Type"
                                                    fieldLabel="Search Input - Analytics Track Type"
                                                    name="./searchInputTrackType">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <internal
                                                            jcr:primaryType="nt:unstructured"
                                                            selected="{Boolean}true"
                                                            text="Internal"
                                                            value="internal"/>
                                                    <button
                                                            jcr:primaryType="nt:unstructured"
                                                            text="Button"
                                                            value="button"/>
                                                    <exit
                                                            jcr:primaryType="nt:unstructured"
                                                            text="Exit"
                                                            value="exit"/>
                                                    <social
                                                            jcr:primaryType="nt:unstructured"
                                                            text="Social"
                                                            value="social"/>
                                                    <input
                                                            jcr:primaryType="nt:unstructured"
                                                            text="Input"
                                                            value="input"/>
                                                </items>
                                            </searchinputtracktype>
                                            <searchsubmittrackid
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    fieldLabel="Search Submit - Analytics Track ID"
                                                    name="./searchSubmitTrackID"/>
                                            <searchsubmitclickid
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    fieldLabel="Search Submit - Analytics Click ID"
                                                    name="./searchSubmitClickID"/>
                                            <searchsubmittracktype
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                                    emptyText="Track Type"
                                                    fieldLabel="Search Submit - Analytics Track Type"
                                                    name="./searchSubmitTrackType">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <internal
                                                            jcr:primaryType="nt:unstructured"
                                                            selected="{Boolean}true"
                                                            text="Internal"
                                                            value="internal"/>
                                                    <button
                                                            jcr:primaryType="nt:unstructured"
                                                            text="Button"
                                                            value="button"/>
                                                    <exit
                                                            jcr:primaryType="nt:unstructured"
                                                            text="Exit"
                                                            value="exit"/>
                                                    <social
                                                            jcr:primaryType="nt:unstructured"
                                                            text="Social"
                                                            value="social"/>
                                                </items>
                                            </searchsubmittracktype>
                                            <searchcanceltrackid
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    fieldLabel="Search Cancel - Analytics Track ID"
                                                    name="./searchCancelTrackID"/>
                                            <searchcancelclickid
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    fieldLabel="Search Cancel - Analytics Click ID"
                                                    name="./searchCancelClickID"/>
                                            <searchcanceltracktype
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                                    emptyText="Track Type"
                                                    fieldLabel="Search Cancel - Analytics Track Type"
                                                    name="./searchCancelTrackType">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <internal
                                                            jcr:primaryType="nt:unstructured"
                                                            selected="{Boolean}true"
                                                            text="Internal"
                                                            value="internal"/>
                                                    <button
                                                            jcr:primaryType="nt:unstructured"
                                                            text="Button"
                                                            value="button"/>
                                                    <exit
                                                            jcr:primaryType="nt:unstructured"
                                                            text="Exit"
                                                            value="exit"/>
                                                    <social
                                                            jcr:primaryType="nt:unstructured"
                                                            text="Social"
                                                            value="social"/>
                                                </items>
                                            </searchcanceltracktype>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </properties>
                    <quicklinks
                            jcr:primaryType="nt:unstructured"
                            jcr:title="Search QuickLinks"
                            sling:resourceType="granite/ui/components/coral/foundation/container"
                            margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                    margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <searchquicklinks
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                                    composite="{Boolean}true"
                                                    fieldLabel="Quick Links for Mobile Search">
                                                <field
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/container"
                                                        name="./searchQuickLinks">
                                                    <items jcr:primaryType="nt:unstructured">
                                                        <Text
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                fieldLabel="QuickLinks Text"
                                                                name="./text"/>
                                                        <ctaurl
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/form/pathbrowser"
                                                                fieldLabel="QuickLinks URL"
                                                                name="./ctaUrl"/>
                                                        <linktarget
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/foundation/include"
                                                                path="aem-airmiles-web/components/includes/mastercomponent/cq:dialog/content/items/tabs/items/properties/items/columns/items/column/items/linktarget"/>
                                                        <analytics
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/include"
                                                                path="aem-airmiles-web/components/includes/mastercomponent/cq:dialog/content/items/tabs/items/properties/items/columns/items/column/items/analytics-options"/>
                                                    </items>
                                                </field>
                                            </searchquicklinks>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </quicklinks>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
